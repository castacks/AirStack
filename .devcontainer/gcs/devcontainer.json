{
  "name": "GCS Container",
  "dockerComposeFile": [
    "../../docker-compose.yaml",
  ],
  "runServices": [
    "gcs"
  ],
  "service": "gcs",
  "workspaceFolder": "/home/gcs/AirStack",
  "initializeCommand": "export AUTOLAUNCH=false && echo 'You may close this window'",
  "customizations": {
    "vscode": {
      "extensions": [
        "Ranch-Hand-Robotics.rde-pack",
        "ms-vscode.cpptools",
        "ms-vscode.cmake-tools",
        "ms-python.black-formatter"
      ]
    }
  }
}