{
  "name": "Robot Container",
  "runServices": [
    "robot"
  ],
  "dockerComposeFile": [
    "../../docker-compose.yaml"
  ],
  "service": "robot",
  "workspaceFolder": "/home/robot/AirStack",
  "initializeCommand": "export AUTOLAUNCH=false && echo 'You may close this window'",
  "customizations": {
    "vscode": {
      "extensions": [
        "Ranch-Hand-Robotics.rde-pack",
        "ms-vscode.cpptools",
        "ms-python.black-formatter"
      ],
    }
  }
}