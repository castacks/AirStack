<launch>
    <arg name="namespace" default="$(env ROBOT_NAME)" />
    <node pkg="tf_relay" exec="tf_relay" name="tf_relay">
        <param name="namespace" value="$(var namespace)" />
    </node>
    <node pkg="tf_relay" exec="tf_static_relay" name="tf_static_relay">
        <param name="namespace" value="$(var namespace)" />
    </node>
</launch>
  